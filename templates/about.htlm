    <!-- Section Partenaires -->
    <section class="partners">
        <div class="container">
            <div class="section-title animate-on-scroll">
                <h2>Nos Partenaires de Confiance</h2>
                <p>Institutions et entreprises qui accompagnent notre vision</p>
